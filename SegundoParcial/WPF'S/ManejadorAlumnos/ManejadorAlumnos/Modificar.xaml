        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Modificar" Height="300" Width="300">
    <Grid>
        <Label x:Name="lbFolio" Content="Folio" HorizontalAlignment="Left" Margin="54,60,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lbCorreo" Content="Correo" HorizontalAlignment="Left" Margin="54,90,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbFolio" HorizontalAlignment="Left" Height="24" Margin="114,62,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbCorreo" HorizontalAlignment="Left" Height="22" Margin="114,94,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btModificar" Content="Modificar" HorizontalAlignment="Left" Margin="114,162,0,0" VerticalAlignment="Top" Width="74" Click="btModificar_Click"/>
        <Button x:Name="btRegresar" Content="Regresar" HorizontalAlignment="Left" Margin="206,238,0,0" VerticalAlignment="Top" Width="76"/>

    </Grid>
</Window>
