        Title="Baja" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="tbFolio" HorizontalAlignment="Left" Height="23" Margin="114,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lbFolio" Content="Folio" HorizontalAlignment="Left" Margin="64,63,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btBaja" Content="Baja" HorizontalAlignment="Left" Margin="114,174,0,0" VerticalAlignment="Top" Width="75" Click="btBaja_Click"/>
        <Button x:Name="btRegresar" Content="Regresar" HorizontalAlignment="Left" Margin="206,238,0,0" VerticalAlignment="Top" Width="76" Click="btRegresar_Click"/>

    </Grid>
</Window>
